    "def string_fact(string):\n",
    "  if '/' in string:\n",
    "    nd = string.split('/')\n",
    "    n = float(nd[0])\n",
    "    d = float(nd[1])\n",
    "    ans = n/d\n",
    "    return ans\n",
    "  else:\n",
    "    ans = float(string)\n",
    "    return ans\n",
    "  \n",
    "nd = input('Enter values: ')\n",
    "string_fact(nd)"
   ]
  }
